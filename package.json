{
  "name": "tabber",
  "version": "1.0.0",
  "description": "* Interactive tabs\r   * Move up/down, delete, add new, give name\r * Audible notes\r * Notations\r   * Hammer on, pull off, slide up, slide down\r * Chord-builder\r * Tuning\r   * With corresponding fretboard notes",
  "main": "server.js",
  "scripts": {
    "server": "nodemon server.js",
    "client": "cd client && npm run start",
    "start": "concurrently \"npm run server\" \"npm run client\"",
    "build": "cd client && npm run build"
  },
  "engines": {
    "node": "14.x"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JonLunde/tabber.git"
  },
  "author": "Jon Lunde",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/JonLunde/tabber/issues"
  },
  "homepage": "https://github.com/JonLunde/tabber#readme",
  "dependencies": {
    "concurrently": "^6.2.0",
    "cors": "^2.8.5",
    "express": "^4.17.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.12"
  }
}
